def getBuddyInfo(iNo, sKey="", uDefault=0):
	if iNo not in gdBuddy:
		raise PlannerError,'没有编号为{}的伙伴'.format(iNo)
	if not sKey:
		return gdBuddy[iNo]
	return gdBuddy[iNo].get(sKey,uDefault)

def getBaseInfo(level):
	if not level:
		level = 1
	if level not in gdBase:
		raise PlannerError("没有等级为%s的伙伴基础能力" % level)
	return gdBase.get(level,{})

def getRelation(relationNo):
	if relationNo not in gdRelation:
		raise PlannerError("没有编号为%s的羁绊数据" % relationNo)
	return gdRelation[relationNo]

def getQuality(quality,sKey,uDefault=0):
	if quality not in gdQuality:
		raise PlannerError,"没有编号为%d的品质数据"% quality
	return gdQuality[quality].get(sKey,uDefault)

def getMajor(score,sKey,uDefault=0):
	data = findSort.getRightValue(score,gdMajor)
	if not data:
		return uDefault
	return data[sKey]

def getCost(times,level,uDefault=0):
	data = findSort.getRightValue(times,gdCost)
	if not data:
		return uDefault

	return data["费用"](level)

def getMax(sKey,level):
	if sKey not in gdMax:
		raise PlannerError,'没有类型为{}的伙伴费用上限'.format(sKey)

	return gdMax[sKey]["费用"](level)

