# -*- coding: utf-8 -*-

def dprint(func):
	def _deco(*args):
		ti = func(*args)
#		print "cost time:%f" % ti, "magicId:%s,shape:%s" % (args[0], args[1])
		return ti
	return _deco

@dprint
def getTime(magicId, shape):
	if magicId < 100: #　非法术
		if shape not in shapeData:
			print "[getMagicTime]not found shape", shape
			return 5
		magicName = war.defines.magicNameList[magicId]
		return shapeData[shape].get(magicName, 0)
	else: #　法术
		if magicId not in performData:
			magicIdBase = magicId / 10 * 10 # 近攻法术默认magicId,如法术1111的默认magicId为11110，没有11111、11112时默认为11110
			if magicIdBase in performData:
				magicId = magicIdBase
			else:
				print "[getMagicTime]not found magicId", magicId
				return 5
		return performData[magicId]["时间"]

