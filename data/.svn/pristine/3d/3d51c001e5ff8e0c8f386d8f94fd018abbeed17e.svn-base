def getPointNext(skillName, level):
	if level not in gdPraticeExp:
		raise PlannerError("没有等级为{}的修炼技能等级数据".format(level))
	return gdPraticeExp[level][skillName]

def getGuildLevelLimit(guildLevel):
	return findSort.getRightValue(guildLevel,gdGuildLevel)["上限"]

def getRoleLevelLimit(guildLevel):
	return findSort.getRightValue(guildLevel,gdRoleLevel)["上限"]

