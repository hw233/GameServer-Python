def getConfig(iNo, sKey="", uDefault=0):
	if iNo not in gdData:
		raise PlannerError,'没有编号为{}的商品'.format(iNo)
	if not sKey:
		return gdData[iNo]
	return gdData[iNo].get(sKey,uDefault)

def hasProps(propsNo):
	return gdData.get(propsNo)

